@model OnlineTicariOtomasyon.Models.Classes.Departman
@{
    ViewBag.Title = "DepartmanEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />
<h2>Yeni Departman Sayfası</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.DepartmanName)
    @Html.TextBoxFor(x => x.DepartmanName, new { @class = "form-control" })
    <br>
    <button class="btn btn-info">Kaydet</button>
</form>
